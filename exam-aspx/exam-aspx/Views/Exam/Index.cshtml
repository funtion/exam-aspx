@{
    ViewBag.Title = "Index";
}
@section css{
    @Styles.Render("~/Content/css/layout.css")
}
<div id="container">
    @Html.Partial("student_header")
    <div id="content">
        <div style="padding-left:160px;padding-right:160px">
            
            <div id="pad20" >
                <h1 align="center"> Name: @ViewBag.exam.name</h1>
                <h1 align="center"> Time @ViewBag.exam.time minutes</h1>
                <h1 align="center"> You can NOT stop the exam after it is stared</h1>
                <div style="text-align:center">
                    <h1>
                        <a href="/Exam/Exam/@ViewBag.exam.id">start exam</a>
                    </h1>
                </div>
               

            </div>
        </div>
    </div>
</div>